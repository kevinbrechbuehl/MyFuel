{
  "name": "dotnet-application",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/backend/application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx-dotnet/core:build",
      "outputs": [
        "{workspaceRoot}/dist/libs/backend/application",
        "{workspaceRoot}/dist/intermediates/libs/backend/application"
      ],
      "options": {
        "configuration": "Debug",
        "noDependencies": true,
        "noRestore": true
      },
      "configurations": {
        "production": {
          "configuration": "Release"
        }
      },
      "defaultConfiguration": "production"
    },
    "lint": {
      "executor": "@nx-dotnet/core:format"
    }
  }
}
