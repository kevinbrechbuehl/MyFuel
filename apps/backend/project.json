{
  "name": "backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/backend",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx-dotnet/core:build",
      "outputs": [
        "{workspaceRoot}/dist/apps/backend",
        "{workspaceRoot}/dist/intermediates/apps/backend",
        "{projectRoot}/bin",
        "{projectRoot}/obj"
      ],
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "options": {
        "configuration": "Debug",
        "noDependencies": true
      },
      "configurations": {
        "production": {
          "configuration": "Release"
        }
      },
      "parallelism": true
    },
    "serve": {
      "executor": "@nx-dotnet/core:serve",
      "options": {
        "configuration": "Debug"
      },
      "configurations": {
        "production": {
          "configuration": "Release"
        }
      },
      "parallelism": true
    },
    "lint": {
      "executor": "@nx-dotnet/core:format",
      "cache": true,
      "inputs": [
        "{projectRoot}/**/*.{cs,fs,vb}"
      ],
      "configurations": {},
      "options": {},
      "parallelism": true
    }
  }
}
